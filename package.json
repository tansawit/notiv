{
  "name": "notis-extension",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock": "vite --config vite.mock.config.ts",
    "build": "tsc --noEmit && vite build",
    "check": "tsc --noEmit",
    "lint": "eslint src --max-warnings=0",
    "format": "prettier --write \"*.{md,json}\" \".github/workflows/*.yml\"",
    "format:check": "prettier --check \"*.{md,json}\" \".github/workflows/*.yml\"",
    "package": "rm -f notis-extension-$npm_package_version.zip && cd dist && zip -r ../notis-extension-$npm_package_version.zip .",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@base-ui/react": "^1.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "dialkit": "^0.2.0",
    "framer-motion": "^12.34.0",
    "@crxjs/vite-plugin": "^2.0.0-beta.33",
    "@eslint/js": "^9.22.0",
    "@types/chrome": "^0.1.36",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^17.3.0",
    "jsdom": "^28.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.26.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
